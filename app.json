{
  "expo": {
    "name": "sanghviTrack",
    "icon": "./assets/icon.png",
    "slug": "sanghviTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sanghvitrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nvm_meet.sanghviTrack",
      "icon": "./assets/icon.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "icon": "./assets/icon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8f3cb197-3d77-4792-b05d-75db3c8ecf6e"
      }
    }
  }
}
